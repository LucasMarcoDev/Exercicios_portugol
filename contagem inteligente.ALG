algoritmo "semnome"
var
   i,f:inteiro
inicio
      escreval("INICIO: ")
      leia(i)
      escreval("FIM: ")
      leia(f)
      enquanto (i <= f)faca
        escreva(i, "...")
        i <- i + 1

      fimenquanto
      enquanto (i > f)faca
           escreva(i, "...")
           i <- i -1
           fimenquanto

      
fimalgoritmo